
@{
    ViewData["Title"] = "News";
}
<title>News</title>
<body>
    <h1><strong>What's Happening Here</strong></h1>
    <br />
    <br />

    <!--News Carousel-->

    <div class="row">
        <div class="col-12">

            <!--Carousel Setup-->

            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                </ol>

                <div class="carousel-inner">
                    <!--Carousel slide 3-->
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="~/Images/Carousel/NewsCarousel/Fakevideo1.png" alt="First slide">
                    </div>

                    <!--Carousel slide 2-->
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/Images/Carousel/NewsCarousel/fake video 2.png" alt="Second slide">
                    </div>

                    <!--Carousel Slide 3-->
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/Images/Carousel/NewsCarousel/fake video 3.png" alt="Third slide">
                    </div>
                </div>

                <!--Carousel controls-->
                <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>

    <br />
    <br />
    <hr.new1 />

    <!--Tables-->
    <div class="row">
        <!--Announcements Column Section-->
        <div class="col">
            <div class="card.new1">
                <div class="card-body text-light">
                    <h4 class="card-title"><strong>Announcements</strong></h4>
                    <hr class="new1" />
                    <p class="card-text"><a href="https://louisville.edu/research/announcements/ildstad-walsh"><strong>Two UofL researchers named Fellows of National Academy of Inventors</strong></a></p>
                    <p class="card-text"><a href="https://louisville.edu/research/announcements/uofl-funding-opp-excite"><strong>UofL Funding Opportunity for Product Development & Research Commercialization</strong></a></p>
                    <p class="card-text"><a href="https://louisville.edu/research/announcements/uofl-nsf-i-corps-funding-opportunity"><strong>UofL NSF I-Corps Funding Opportunity</strong></a></p>
                    <a href="https://louisville.edu/research/announcements" class="btn btn-danger">Announcements</a>
                </div>
            </div>
        </div>
    </div>


    <br />
    <br />


    <!--Tables-->
    <div class="row">
        <!--News Feed Section  Column-->
        <div class="col">
            <div class="card.new1">
                <div class="card-body text-light">
                    <h4 class="card-title"><strong>Newsfeed</strong></h4>
                    <hr class="new1" />
                    <p class="card-text"><a href="http://www.uoflnews.com/section/science-and-tech/uofl-born-company-secures-an-additional-115-million-to-advance-cell-therapy/"><strong>UofL-born company secures an additional $115 million to advance cell therapy</strong></a></p>
                    <p class="card-text"><a href="http://www.uoflnews.com/post/uofltoday/uofl-to-launch-health-care-cybersecurity-curriculum-with-6-3m-from-national-security-agency/"><strong>UofL to launch health care cybersecurity curriculum with $6.3M from NSA</strong></a></p>
                    <p class="card-text"><a href="http://www.uoflnews.com/post/uofltoday/uofl-launches-48-hour-student-innovation-challenge/"><strong>UofL launches 48-hour student innovation challenge</strong></a></p>
                    <a href="https://mailchi.mp/louisville/newsletter-signup" class=" btn btn-danger">Subscribe to our UofL Innovation Newsletter  </a>
                </div>
            </div>
        </div>

    </div>
    <br />
    <br />

    <div id="fb-root"></div>
    <script async defer crossorigin="anonymous" src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v8.0" nonce="xEmggBxy"></script>



    <!--Tables-->
    <div class="row">
        <!--Social Media-->
        <div class="col">
            <div class="card.new1">
                <div class="card-body text-light">
                    <h4 class="card-title"><strong>Social Media</strong> </h4>
                    <hr class="new1" />
                    <p class="card-text"><a href="https://www.facebook.com/UofLInnovate/"><strong>Facebook</strong></a></p>
                    <p class="card-text"><a href="https://www.instagram.com/uofu_our/"><strong>Instagram</strong></a></p>
                    <a href="https://twitter.com/uoflinnovate?ref_src=twsrc%5Etfw" class="twitter-follow-button" data-show-count="false"><strong>Follow @@uoflinnovate</strong></a>
                    <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>

                </div>
            </div>
        </div>

    </div>
    <br />
    <br />


</body>

