
@{
    ViewData["Title"] = "News";
}
<title>News</title>
<body>
    <h1><strong>What's Happening Here</strong></h1>
    <br />
    <br />

    <!--News Carousel-->

    <div class="row">
        <div class="col-12">

            <!--Carousel Setup-->

            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                </ol>

                <div class="carousel-inner">
                    <!--Carousel slide 3-->
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="~/Images/Carousel/NewsCarousel/Fakevideo1.png" alt="First slide">
                    </div>

                    <!--Carousel slide 2-->
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/Images/Carousel/NewsCarousel/fake video 2.png" alt="Second slide">
                    </div>

                    <!--Carousel Slide 3-->
                    <div class="carousel-item">
                        <img class="d-block w-100" src="~/Images/Carousel/NewsCarousel/fake video 3.png" alt="Third slide">
                    </div>
                </div>

                <!--Carousel controls-->
                <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>

    <br />
    <br />
    <hr.new1 />

    <a href="https://mailchi.mp/louisville/newsletter-signup" class=" btn btn-primary" style="font-size:30px; text-align:center;">Subscribe to our UofL Innovation Newsletter  </a> 
    <br />
    <br />
    <br/>

    <!--Tables-->
    <div class="row">
        <!--Announcements Column Section-->
        <div class="col">
            <div class="card.new1">
                <div class="card-body text-light">
                    <h4 class="card-title"><strong>Announcements</strong></h4>
                    <hr class="new1" />
                    <p class="card-text"><a href="https://louisville.edu/research/announcements/ildstad-walsh">Two UofL researchers named Fellows of National Academy of Inventors></a></p>
                    <p class="card-text"><a href="https://louisville.edu/research/announcements/uofl-funding-opp-excite">UofL Funding Opportunity for Product Development & Research Commercialization</a></p>
                    <p class="card-text"><a href="https://louisville.edu/research/announcements/uofl-nsf-i-corps-funding-opportunity">UofL NSF I-Corps Funding Opportunity</a></p>
                    <a href="https://louisville.edu/research/announcements" class="btn btn-primary">Announcements</a>
                </div>
            </div>
        </div>
    </div>


    <br />
    <br />


    <!--Tables-->
    <div class="row">
        <!--News Feed Section  Column-->
        <div class="col">
            <div class="card.new1">
                <div class="card-body text-light">
                    <h4 class="card-title"><strong>Newsfeed</strong></h4>
                    <hr class="new1" />
                    <p class="card-text"><a href="http://www.uoflnews.com/section/science-and-tech/uofl-born-company-secures-an-additional-115-million-to-advance-cell-therapy/">UofL-born company secures an additional $115 million to advance cell therapy</a></p>
                    <p class="card-text"><a href="http://www.uoflnews.com/post/uofltoday/uofl-to-launch-health-care-cybersecurity-curriculum-with-6-3m-from-national-security-agency/">UofL to launch health care cybersecurity curriculum with $6.3M from NSA</a></p>
                    <p class="card-text"><a href="http://www.uoflnews.com/post/uofltoday/uofl-launches-48-hour-student-innovation-challenge/">UofL launches 48-hour student innovation challenge</a></p>
                    <a href="https://mailchi.mp/louisville/newsletter-signup" class=" btn btn-primary">Read More Now  </a>
                </div>
            </div>
        </div>

    </div>
    <br />
    <br />

    <div id="fb-root"></div>
    <script async defer crossorigin="anonymous" src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v8.0" nonce="xEmggBxy"></script>



    <!--Tables-->
    <div class="row">
        <!--Social Media-->
        <div class="col">
            <div class="card.new1">
                <div class="card-body text-light">
                    <h4 class="card-title"><strong>Social Media</strong> </h4>
                    <hr class="new1" />
                    <p class="card-text" style="font-size:20px;"><a class="external-link" href="https://www.facebook.com/UofLInnovate" target="_blank" title="Facebook - UofL Innovate"><img src="https://louisville.edu/research/images/facebook" alt="Facebook" class="image-inline" title="Facebook"> - Like us on Facebook</a></p>
                    <p class="card-text" style="font-size:20px;"><a class="external-link" href="https://www.linkedin.com/company/15205874" target="_blank" title="LinkedIn - UofL Innovate"><img src="https://louisville.edu/research/images/ln30.png" alt="LinkedIn" class="image-inline" title=""> - Connect with us on LinkedIn</a></p>
                    <p class="card-text" style="font-size:20px;"><a class="external-link" href="https://twitter.com/UofLInnovate" target="_blank" title="Twitter - UofL Innovate"><img src="https://louisville.edu/research/images/twitter" alt="Twitter Logo" class="image-inline" title="Twitter"> - Follow us on Twitter</a></p>


                </div>
            </div>
        </div>

    </div>
    <br />
