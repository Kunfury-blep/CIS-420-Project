
@{
    ViewData["Title"] = "Student Home Page";
}

<h1>Students Home Page</h1>

<style>
* {
  box-sizing: border-box;
}

body {
  font-family: Arial, Helvetica, sans-serif;
}

.header {
  background-color: #f1f1f1;
  padding: 30px;
  text-align: center;
  font-size: 35px;
}

.column {
  float: left;
  width: 33.33%;
  padding: 10px;
  height: 600px; 
}


.row:after {
  content: "";
  display: table;
  clear: both;
}

</style>

<div class="row">
    <div class="column" style="background-color:#aaa;color:black;">iRIS</div>
    <div class="column" style="background-color:#bbb;color:black;">Student Carousel</div>
    <div class="column" style="background-color:#ccc;color:black;">Links</div>
</div>


